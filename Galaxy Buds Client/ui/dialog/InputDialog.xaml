<Window x:Class="Galaxy_Buds_Client.ui.dialog.InputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="{DynamicResource settings_cpopup_override_title}" Height="176" Width="304"
        ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterOwner"
        >
    <Grid>
        <Button Content="{DynamicResource apply}" Margin="0,0,10,10" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Apply_OnClick"/>
        <Button Content="{DynamicResource cancel}" Margin="0,0,93,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Cancel_OnClick" Height="25"/>
        <TextBox x:Name="Input" Margin="10,0,10,45" Background="#FFE6E6E6" Height="22" VerticalAlignment="Bottom" IsReadOnly="False"/>
        <TextBlock Text="{DynamicResource settings_cpopup_override_title_dialog}" TextWrapping="Wrap"
                   HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="278" Height="65"/>
    </Grid>
</Window>
