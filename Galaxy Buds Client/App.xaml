<Application x:Class="Galaxy_Buds_Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Galaxy_Buds_Client"
             Startup="App_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Brushes" Source="/ui/themes/Brushes.xaml"/>
                <ResourceDictionary x:Name="WindowStyle" Source="/ui/basewindow/SWStyles.xaml"/>
                <ResourceDictionary x:Name="MainStyle" Source="/ui/themes/Styles.xaml"/>
                <ResourceDictionary x:Name="TrayStyle" Source="/ui/themes/TrayContextStyles.xaml"/>
                <ResourceDictionary x:Name="SliderStyle" Source="/ui/element/Slider.xaml"/>
                <ResourceDictionary x:Name="CurrentLocale" Source="/i18N/en.xaml"/>
                <ResourceDictionary x:Name="FallbackLocale" Source="/i18N/en.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
